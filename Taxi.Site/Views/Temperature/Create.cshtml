@model IEnumerable<Taxi.DataAccessLayer.Entites.Humidity>

@{
	ViewData["Title"] = "‌اضافه کردن  امتیاز ";
	Layout = "~/Views/Shared/AdminPanel/_AdminPanelLayout.cshtml";
}
<div class="content-wrapper">
	<!-- Content -->

	<div class="container-xxl flex-grow-1 container-p-y">
		<div class="row">
			<div class="col-xl">
				<div class="card mb-4">
					<div class="card-header d-flex justify-content-between align-items-center">
						<small class="text-muted float-end">اضافه کردن قیمت بر اساس رطوبت  </small>
					</div>
					<div class="card-body">
						<form method="post" asp-action="Create" asp-controller="Temperature">
							<div class="mb-3">
								<label class="form-label" for="basic-default-fullname">نام </label>
								<input name="Name" type="text" class="form-control" id="basic-default-fullname"  />
							</div>
							<div class="mb-3">
								<label class="form-label" for="basic-default-fullname"> از دما</label>
								<input name="Start" type="number" class="form-control" id="basic-default-fullname" />
							</div>
							<div class="mb-3">
								<label class="form-label" for="basic-default-fullname">تا دما </label>
								<input name="End" type="number" class="form-control" id="basic-default-fullname" />
							</div>
							<div class="mb-3">
								<label class="form-label" for="basic-default-fullname"> درصد افزایش قیمت</label>
								<input name="Precent" step="0.1" type="number" class="form-control" id="basic-default-fullname" />
							</div>
							<button type="submit" class="btn btn-primary">ثبت</button>
						</form>
					</div>
				</div>
			</div>

		</div>
	</div>
</div>

